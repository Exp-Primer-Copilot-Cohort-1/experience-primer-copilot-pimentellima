{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [
            "off",
            4
        ],
        "class-methods-use-this": "off",
        "no-undef": "off",
        "no-param-reassign": "off",
        "no-return-await": "off",
        "strict": "off",
        "object-curly-spacing": "off",
        "camelcase": "off",
    }
}
